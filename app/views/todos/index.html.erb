<h1>Todo list</h1>
 
<table>
  <tr>
    <th>Text</th>
    <th>IsCompleted</th>
    <th></th>
  </tr>
 
  <% @todos.each do |todo| %>
    <tr>
      <td><%= todo.text %></td>
      <td><%= todo.isCompleted %></td>
      <!--<td><%= link_to 'Show', todo_path(todo) %></td>-->
    </tr>
  <% end %>
</table>
